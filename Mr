								v.Name = NAME
							end
						end
					end
				elseif game:GetService("Players").LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 and v.Handle.TextureId == HatID  then
					if v.Name == rname then
						if oname == "Hat" then
							v.Name = NAME
