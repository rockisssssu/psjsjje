local baseUrl = "https://github.com/MrBwoken/Music-/raw/refs/heads/main/KrystalDanceV3/"
local files = {
    "Down.mp3", "Moongazer.mp3", "Rewind.mp3", "Slickback.mp3", "billy.mp3",
    "bimbam.mp3", "boogie.mp3", "chronoshift.mp3", "dr.wav", "egypt.mp3",
    "faster.ogg", "goat.mp3", "griddy.mp3", "lux.ogg", "mystery.mp3",
    "order.mp3", "part.MP3", "pogo.mp3", "rat.mp3", "secret.mp3",
    "sturdy.mp3", "true_heart.mp3", "tryna.mp3", "valen.mp3", "xav.mp3"
}

local allFilesExist = true
local downloadThreads = {}

for _, filename in ipairs(files) do
    if not isfile(filename) then
        allFilesExist = false
        local thread = coroutine.create(function()
            local url = baseUrl .. filename
            local success, result = pcall(function()
                return game:HttpGet(url)
            end)

            if success then
                writefile(filename, result)
                print("Downloaded:", filename)
            else
                print("Failed to download:", filename)
            end
        end)
        table.insert(downloadThreads, thread)
        coroutine.resume(thread)
    else
        print("Skipping existing file:", filename)
    end
end

for _, thread in ipairs(downloadThreads) do
    coroutine.resume(thread)
end

if not allFilesExist then
    local CoreGui = game:GetService("CoreGui")
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")

    ScreenGui.Parent = CoreGui
    Frame.Parent = ScreenGui
    Frame.Size = UDim2.new(0.3, 0, 0.1, 0)
    Frame.Position = UDim2.new(0.35, 0, -0.1, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    Frame.BackgroundTransparency = 0.3
    Frame.BorderSizePixel = 0
    Frame.ClipsDescendants = true
    Frame:TweenPosition(UDim2.new(0.35, 0, 0.05, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 1, true)

    TextLabel.Parent = Frame
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Text = "All songs downloaded!"
    TextLabel.TextScaled = true
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Font = Enum.Font.SourceSansBold

    task.wait(5)
    Frame:TweenPosition(UDim2.new(0.35, 0, -0.1, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quad, 1, true)
    task.wait(1)
    ScreenGui:Destroy()
end

print("Continuing script execution...")




