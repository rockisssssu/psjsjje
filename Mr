				child.Anchored = false
			elseif child:IsA("Accessory") then
				child.Handle.Anchored = false
			end
		end
	end)
	task.spawn(function()
		wait(game:GetService("Players").RespawnTime - .05)
		task.wait(.05)
		velocity:Disconnect() 
		connect:Disconnect()
	end)
end
